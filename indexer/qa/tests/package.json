{
  "name": "midnight-indexer-viteset",
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx,graphql,gql}'",
    "audit": "yarn npm audit && echo 'No known vulnerabilities found.'",
    "test": "vitest run",
    "test:smoke": "vitest run smoke",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean:logs": "rm -rf logs/",
    "clean:reports": "rm -rf reports/",
    "clean:modules": "rm -rf node_modules/",
    "clean": "yarn clean:logs && yarn clean:reports && yarn clean:modules"
  },
  "resolutions": {
    "graphql": "16.11.0"
  },
  "devDependencies": {
    "@graphql-inspector/core": "^6.2.1",
    "@types/node": "^22.15.19",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bech32": "^2.0.0",
    "diff": "^8.0.2",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "graphql-ws": "^6.0.5",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "ws": "^8.18.2",
    "xmlbuilder2": "^3.1.1",
    "zod": "^3.25.32"
  }
}
