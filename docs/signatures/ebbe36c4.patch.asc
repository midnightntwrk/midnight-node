diff --git a/ledger/src/transaction.rs b/ledger/src/transaction.rs
index 31fa22cc..5b2c8ead 100644
- --- a/ledger/src/transaction.rs
+++ b/ledger/src/transaction.rs
@@ -17,6 +17,7 @@ impl From<mn_ledger::error::MalformedTransaction> for MalformedError {
 	}
 }
 
+#[derive(Clone, Debug, PartialEq, Eq)]
 pub struct Transaction(Tx<Proof>);
 
 impl Transaction {
@@ -48,3 +49,34 @@ impl Transaction {
 			.collect()
 	}
 }
+
+#[cfg(test)]
+mod tests {
+	use super::*;
+	#[test]
+	fn should_validate_transaction() {
+		let bytes = include_bytes!("../test_data/wellformed_tx.json");
+		let tx = Transaction::from_bytes(bytes);
+		assert!(tx.is_ok(), "Deserialization failed with: {}", tx.unwrap_err());
+		let result = tx.unwrap().validate();
+		assert!(result.is_ok(), "Validation failed with: {}", result.unwrap_err());
+	}
+
+	#[test]
+	fn should_fail_to_deserialize_transaction() -> Result<(), String> {
+		let bytes = "Invalid Tx".as_bytes();
+		match Transaction::from_bytes(bytes) {
+			Err(_) => Ok(()),
+			Ok(_) => Err(String::from("Deserialization should have failed")),
+		}
+	}
+
+	#[test]
+	fn should_not_validate_malformed_transactin() -> Result<(), String> {
+		let bytes = include_bytes!("../test_data/malformed_tx.json");
+		match Transaction::from_bytes(bytes) {
+			Err(_) => Ok(()),
+			Ok(_) => Err(String::from("Validation should have failed")),
+		}
+	}
+}
